<div class="container">
  <div class="row">
    <div class="col s10 offset-1">

      <div id="profilePic">
        <img class="responsive-img" src="google.com" alt="Profile Picture"/>
          <br>
        <h2><%= @user.username %></h2>
      </div>

    <div class="card small">
      <div class="card-image">
        <img src="#" alt="Restaurant Image">
        <span class="card-title">Restaurant Name</span>
      </div>
        <div class="card-content">
          <p>Cuisines</p>
          <p>Price Range</p>
        </div>
        <div class="card-action">
          <a href="#">This is a link</a>
        </div>
      </div>
    </div>
  </div>
</div>
