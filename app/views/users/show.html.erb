<div class="container">
  <div class="row"
    <div class="col s10 offset-1">
      <div id="profilePic">
      <img class="responsive-img" src="google.com"/>
      <br>
      <h2><%= @user.username %></h2>
    </div>
      <div class="card small">
        <div class="card-image">
          <img src="<%= @restaurants.featured_image %>">
            <span class="card-title"><%= @restaurants.name %></span>
            </div>
          <div class="card-content">
            <p><%= @restaurants.cuisines %></p>
            <p><%= @restaurants.price_range %></p>
          </div>
            <div class="card-action">
            <a href="#">This is a link</a>
          </div>
        </div>
      </div>
    </div>
  </div>
